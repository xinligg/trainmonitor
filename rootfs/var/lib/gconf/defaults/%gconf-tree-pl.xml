				</entry>
				<entry name="window_height">
					<local_schema short_desc="Wysokość okna w pikselach">
					</local_schema>
				</entry>
				<entry name="window_width">
					<local_schema short_desc="Szerokość okna w pikselach">
					</local_schema>
