      <key>/schemas/apps/compiz/plugins/scale/allscreens/options/initiate_all_key</key>
      <applyto>/apps/compiz/plugins/scale/allscreens/options/initiate_all_key</applyto>
      <owner>compiz</owner>
      <type>string</type>
