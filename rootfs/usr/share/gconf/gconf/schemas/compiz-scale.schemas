      <key>/schemas/apps/compiz/plugins/scale/allscreens/options/initiate_output_edge</key>
      <applyto>/apps/compiz/plugins/scale/allscreens/options/initiate_output_edge</applyto>
      <owner>compiz</owner>
      <type>string</type>
